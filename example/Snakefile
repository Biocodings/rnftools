import smbl
#include: smbl.include

import mishmash
include: mishmash.include1

fe=smbl.fasta.EXAMPLE

name="test"

mishmash.ArtIllumina(name=name, fasta=fe,coverage=1)
mishmash.DwgSim(name=name, fasta=fe,coverage=1)
mishmash.DwgSim(name=name, fasta=fe,read_length_1=100,read_length_2=100,coverage=1)

include: mishmash.include2

rule basic:
	input:
		#fe,
		mishmash.joined_fq_fn(name)
