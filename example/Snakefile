import smbl
import mishmash

#fe=smbl.fasta.HUMAN_HG38
fe=smbl.fasta.EXAMPLE

# problem -- lomitko na konci
mishmash.ArtIllumina(fa=fe,number_of_reads=10000,read_length_1=100,read_length_2=0)
mishmash.DwgSim(fa=fe,number_of_reads=10000)
mishmash.DwgSim(fa=fe,read_length_1=100,read_length_2=100,number_of_reads=10000)

include: mishmash.include

print(mishmash.output)

rule basic:
	input:
		mishmash.output


print(mishmash._SAMPLES_)

